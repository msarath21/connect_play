{
  "expo": {
    "name": "Game Collection",
    "slug": "game-collection",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#673AB7"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourusername.gamecollection",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan QR codes."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#673AB7"
      },
      "package": "com.gamecollection.tictactoe",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Game Collection to access your camera to scan QR codes."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "2636ea7d-dd02-4a24-859d-e3b0c6ba73e8"
      },
      "router": {
        "origin": false
      }
    },
    "newArchEnabled": true,
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "owner": "msarath"
  }
}
